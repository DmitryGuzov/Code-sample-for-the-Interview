  "description": "interview code example",
  "main": "server.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "install": "npm install pm2 -g",
    "start": "npm run clean:logs && pm2-runtime start ecosystem.config.js",
    "start:production": "npm run clean:logs && pm2-runtime ecosystem.config.js --env production",
    "start:prod": "npm run clean:logs && pm2-runtime ecosystem.config.js --env prod",
    "start:staging": "npm run clean:logs && pm2-runtime ecosystem.config.js --env staging",
    "start:dev": "env-cmd -f .env.development nodemon --max-old-space-size=8192 -r dotenv/config server.js"
  },
